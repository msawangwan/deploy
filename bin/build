#!/bin/sh

echo "ci.io build phase ..."
echo

# create the webhook server and container

cd lib/

ls -al

docker system prune -f
docker build -t ci.io.srv:dev .
docker run --rm -d -p 9001:80 --name ci.io.srv ci.io.srv:dev
#docker run --rm -d -p 9001:80 --name ci.io.srv --link ci.io.root ci.io.srv:dev
# docker run --rm -d -p 9001:80 --name ci.io.srv ci.io.srv:dev
# docker run --rm -d -p 9001:80 --name ci.io.srv -v /var/run/docker.sock:/var/run/docker.sock ci.io.srv:dev
